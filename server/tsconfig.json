{
  "compilerOptions": {
    "lib": [
      "es6",
      "dom"
    ],
    "noImplicitAny": true,
    "allowJs": true,
    "target": "es5",
    "strict": true,
    "module": "es6",
    "moduleResolution": "node",
    "sourceMap": false,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true
  }
}
