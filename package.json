{
  "private": true,
  "name": "overland-tracker-ui",
  "productName": "Overland Tracker",
  "main": "dist/src/main.js",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build:app:vite": "cd app && tsc && vite build",
    "build:app": "tsc -p ./app/server/tsconfig.json",
    "start:app": "yarn build:app && node ./app/server/dist/server/index.js",
    "build": "yarn build:app:vite && tsc",
    "watch": "tsc -w",
    "lint": "eslint -c .eslintrc --ext .ts ./src",
    "start": "yarn build && electron .",
    "dev": "yarn build && electron . --debug",
    "package": "yarn package:mac && yarn package:win && package:linux",
    "package:mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --out=out --icon=assets/app-icon/mac/app.icns --osx-sign.identity='Developer ID Application: GitHub' --extend-info=assets/mac/info.plist",
    "package:m1": "electron-packager . --overwrite --platform=darwin --arch=arm64 --out=out --icon=assets/app-icon/mac/app.icns --osx-sign.identity='Developer ID Application: GitHub' --extend-info=assets/mac/info.plist",
    "package:win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --out=out --icon=assets/app-icon/win/app.ico",
    "package:linux": "electron-packager . --overwrite --platform=linux --arch=x64 --out=out"
  },
  "dependencies": {
    "@reshadow/react": "^0.0.1-issue-119.0",
    "chokidar": "^3.5.1",
    "electron-store": "^8.0.0",
    "electron-window-state": "^5.0.3",
    "jspath": "^0.4.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-hooks-sse": "^2.0.0",
    "ticks-to-date": "^1.0.3",
    "vite": "^2.0.4",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/jspath": "^0.4.0",
    "@types/node": "^14.14.27",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/xml2js": "^0.4.8",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "cors": "^2.8.5",
    "electron": "^14.0.0",
    "electron-packager": "^15.4.0",
    "eslint": "^7.20.0",
    "express": "^4.17.1",
    "typescript": "^4.1.5"
  }
}
